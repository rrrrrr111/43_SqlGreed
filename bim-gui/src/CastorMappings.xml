<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<mapping>
    <description></description>
    <class name="ru.roman.bim.gui.pane.main.MainViewModel">
        <map-to xml="MainViewModel"/>
        <field name="id" type="long">
            <bind-xml name="id" node="element"/>
        </field>
        <field name="facedLangId" type="long">
            <bind-xml name="facedLangId" node="element"/>
        </field>
        <field name="modelNum" type="long">
            <bind-xml name="modelNum" node="element"/>
        </field>
        <field name="category" type="long">
            <bind-xml name="category" node="element"/>
        </field>
        <field name="owner" type="long">
            <bind-xml name="owner" node="element"/>
        </field>
        <field name="rating" type="long">
            <bind-xml name="rating" node="element"/>
        </field>
        <field name="shadowedLangId" type="long">
            <bind-xml name="shadowedLangId" node="element"/>
        </field>
        <field name="textFaced" type="string">
            <bind-xml name="textFaced" node="element"/>
        </field>
        <field name="textShadowed" type="string">
            <bind-xml name="textShadowed" node="element"/>
        </field>
        <field name="type" type="long">
            <bind-xml name="type" node="element"/>
        </field>
    </class>

    <class name="ru.roman.bim.gui.pane.settings.SettingsViewModel">
        <map-to xml="SettingsViewModel"/>
        <field name="cacheMaxSize" type="long">
            <bind-xml name="cacheMaxSize" node="element"/>
        </field>
        <field name="currentNum" type="long">
            <bind-xml name="currentNum" node="element"/>
        </field>
        <field name="facedLangId" type="long">
            <bind-xml name="facedLangId" node="element"/>
        </field>
        <field name="id" type="long">
            <bind-xml name="id" node="element"/>
        </field>
        <field name="login" type="string">
            <bind-xml name="login" node="element"/>
        </field>
        <field name="lookAndFeel" type="string">
            <bind-xml name="lookAndFeel" node="element"/>
        </field>
        <field name="opacity" type="double">
            <bind-xml name="opacity" node="element"/>
        </field>
        <field name="password" type="string">
            <bind-xml name="password" node="element"/>
        </field>
        <field name="portion" type="long">
            <bind-xml name="portion" node="element"/>
        </field>
        <field name="previewDuration" type="long">
            <bind-xml name="previewDuration" node="element"/>
        </field>
        <field name="previewInterval" type="long">
            <bind-xml name="previewInterval" node="element"/>
        </field>
        <field name="ratings" type="int" collection="arraylist">
            <bind-xml name="ratings" node="element"/>
        </field>
        <field name="recordsCount" type="long">
            <bind-xml name="recordsCount" node="element"/>
        </field>
        <field name="shadowedLangId" type="long">
            <bind-xml name="shadowedLangId" node="element"/>
        </field>
        <field name="sortingDirection" type="string">
            <bind-xml name="sortingDirection" node="element"/>
        </field>
        <field name="sortingField" type="string">
            <bind-xml name="sortingField" node="element"/>
        </field>
        <field name="subscribed" type="long" collection="arraylist">
            <bind-xml name="subscribed" node="element"/>
        </field>
        <field name="categories" type="long" collection="arraylist">
            <bind-xml name="categories" node="element"/>
        </field>
        <field name="types" type="long" collection="arraylist">
            <bind-xml name="types" node="element"/>
        </field>
        <field name="workWithPortion" type="boolean">
            <bind-xml name="workWithPortion" node="element"/>
        </field>
    </class>

</mapping>